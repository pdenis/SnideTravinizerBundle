<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="snide_travinizer.github_helper.class">Snide\Bundle\TravinizerBundle\Helper\GithubHelper</parameter>
        <parameter key="snide_travinizer.packagist_helper.class">Snide\Bundle\TravinizerBundle\Helper\PackagistHelper</parameter>
        <parameter key="snide_travinizer.poser_helper.class">Snide\Bundle\TravinizerBundle\Helper\PoserHelper</parameter>
        <parameter key="snide_travinizer.scrutinizer_helper.class">Snide\Bundle\TravinizerBundle\Helper\ScrutinizerHelper</parameter>
        <parameter key="snide_travinizer.travis_helper.class">Snide\Bundle\TravinizerBundle\Helper\TravisHelper</parameter>
    </parameters>

    <services>
        <service id="snide_travinizer.github_helper" class="%snide_travinizer.github_helper.class%" public="true"/>
        <service id="snide_travinizer.packagist_helper" class="%snide_travinizer.packagist_helper.class%" public="true"/>
        <service id="snide_travinizer.poser_helper" class="%snide_travinizer.poser_helper.class%" public="true"/>
        <service id="snide_travinizer.scrutinizer_helper" class="%snide_travinizer.scrutinizer_helper.class%" public="true"/>
        <service id="snide_travinizer.travis_helper" class="%snide_travinizer.travis_helper.class%" public="true"/>
    </services>
</container>